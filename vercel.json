{
  "version": 2,
  "public": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).mov",
      "headers": [
        {
          "key": "Content-Type",
          "value": "video/quicktime"
        }
      ]
    }
  ]
}
